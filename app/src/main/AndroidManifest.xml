        <activity android:name=".view.MainActivity" />
        <activity android:name=".view.DetailActivity" /> <!-- <activity android:name="cosmic.com.mapprjkotlin.view.DataFragment" /> -->
        <activity android:name=".view.LoginActivityKotlin">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>
